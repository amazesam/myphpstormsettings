<keymap version="1" name="Default copy" parent="$default">
  <action id="ActivateFindToolWindow" />
  <action id="ActivateStructureToolWindow">
    <keyboard-shortcut first-keystroke="alt 7" />
    <keyboard-shortcut first-keystroke="alt 3" />
  </action>
  <action id="EditorLineEnd">
    <keyboard-shortcut first-keystroke="end" />
    <keyboard-shortcut first-keystroke="ctrl back_slash" />
  </action>
  <action id="PublishGroup.SyncLocalVsRemoteWith">
    <keyboard-shortcut first-keystroke="alt s" second-keystroke="k" />
  </action>
  <action id="PublishGroup.Upload">
    <keyboard-shortcut first-keystroke="shift ctrl o" />
  </action>
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="ctrl alt l" />
    <keyboard-shortcut first-keystroke="alt d" second-keystroke="f" />
  </action>
  <action id="ShowSettings">
    <keyboard-shortcut first-keystroke="ctrl alt s" />
    <keyboard-shortcut first-keystroke="ctrl alt k" />
  </action>
</keymap>